.registration-bottom-text {
    color: #666;
    font-family: 'Open Sans', sans-serif;
    font-size: 16px;
    font-weight: 400;
    padding: 10px 0px 10px 0px;
}
.registration-bottom-text a {
    color: #006633;
    font-family: 'Open Sans', sans-serif;
    font-size: 16px;
    font-weight: 400;
    text-decoration: none;
}
.form-check.form-check-inline {
    display: inline-block;
}
form.contact-form-registration {
    background-color: #fff;
    padding: 0px;
  /* box-shadow: 0 1px 15px 1px rgb(0 0 0 / 0%), 0 1px 6px rgb(0 0 0 / 5%);*/
    margin-top: 25px;
    padding-left: 38px;
}
.form-control{ webkit-box-shadow: none!important;
box-shadow: none!important;}

footer .footer-bottom-text {
    width: 100%;
    float: left;
    padding: 45px 0;
    border-bottom: 1px solid #66666633 !important;
}
footer .footer-bottom-text .footer-contact-info {
    margin-top: 35px;
    float: left;
}
.footer-bottom-text .footer-contact-logo {
    float: left;
}
.footer-contact-info h4 {
    font-size: 32px;
    color: #333;
   /* text-transform: capitalize;*/
    font-weight: 600;
}
.green-call-button:hover {
    background: #ff0000;
    color: #fff;
    text-decoration: none;
   
}
.green-call-button {
    background: #006633;
    padding: 19px 53px;
    border-radius: 0px;
    color: #fff;
    font-size: 20px;
    font-weight: 400;
    margin: 40px 0 0;
}
.footer-button-text {
    float: left;
    margin-left: 80px;
    margin-top: 45px;
}

/*bottom footer*/
h4.footer-title {
    font-size: 18px;
    font-weight: 400!important;
    color: #333;
    margin: 0 0 20px;
}
footer .footer-link-section {
    width: 100%;
    float: left;
    padding: 45px 0;
    border-bottom: 1px solid #66666633 !important;
}
footer .footer-link-section ul {
    margin: 0;
    padding: 0;
    list-style: none;
    position: relative;
}
footer .footer-link-section ul li a {
    color: #333;
    font-size: 14px;
    line-height: 2em;
    font-family: 'Open Sans', sans-serif;
}
footer .footer-link-section ul li a:hover {
    text-decoration: none !important;
    color:#006633;
}
ul.social-media-link li {
    display: inline-block;
    margin-right: 5px;
}
.fab, .far {
    font-weight: 400;
    font-size: 18px;
}
footer .copyright-section {
    width: 100%;
    float: left;
    padding: 20px 0;
}
footer .copyright-section p {
   float: left
    color: #666;
    margin: 10px 0;
    line-height: 24px;
    font-size:12px !important;
}


