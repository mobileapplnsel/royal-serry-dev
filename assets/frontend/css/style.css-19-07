    margin: 0 auto;
    display: block;
    width: 70%;
}
.footer-button-text {
    margin-left: 0px;
    margin-top: 56px;
}
.green-call-button {
    padding: 19px 30px;}
 footer .footer-bottom-text .footer-contact-info {
    margin-top: 42px;
}   
.footer-contact-info h4 {
    font-size: 19px;
	}
	footer .copyright-section {
    padding: 0px 0;
}
